[core]
	editor = vim
	excludesfile = /Users/chris/.dotfiles/.gitignore_global
	quotepath = false
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[color]
    ui = auto
[github]
    user = Nitron
[credential]
    helper = osxkeychain
[include]
    path = ~/.localrc/.gitconfig
[user]
	name = Chris Williams
	email = chris@christopher-williams.net
[url "https://github.com/"]
	insteadOf = git://github.com/
